server=8.8.8.8
server=8.8.4.4

dhcp-option=3 #Disable default router gateway
dhcp-option=6 # Disables DNS

# OPN
dhcp-host=$WLAN_OPN,$MAC_OPN,$IP_OPN.1
dhcp-range=$IP_OPN.2,$IP_OPN.100,24h

# OPN FREE
dhcp-host=$WLAN_OPN_HIDDEN,$MAC_OPN_HIDDEN,$IP_OPN_HIDDEN.1
dhcp-range=$IP_OPN_HIDDEN.2,$IP_OPN_HIDDEN.100,24h

# PSK
